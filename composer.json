{
    "name": "themsaid/laravel-model-transformers",
    "description": "Easy transformation layer for complex model attributes.",
    "keywords": ["laravel", "API", "JSON"],
    "homepage": "https://github.com/themsaid/laravel-model-transformers",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "theMohamedSaid@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\Transformers\\": "src"
        },
        "files": [
            "src/helpers.php"
        ],
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themsaid\\Transformers\\TransformersServiceProvider"
            ]
        }
    }
}
